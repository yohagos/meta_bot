{
  "name": "meta-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "gen-api": "ng-openapi-gen --input ./openapi/openapi.json --output ./src/app/services && npm run fix-api-types",
    "fix-api-types": "sed -i 's/export {/export type {/g' ./src/app/services/models.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/cdk": "^19.1.3",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/material": "^19.1.3",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@auth0/angular-jwt": "^5.2.0",
    "echarts": "^5.6.0",
    "keycloak-angular": "^19.0.2",
    "keycloak-js": "^26.2.0",
    "lodash": "^4.17.21",
    "ng-openapi-gen": "^0.52.0",
    "rxjs": "~7.8.0",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "19.0.0",
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular/cli": "^19.1.6",
    "@angular/compiler-cli": "^19.1.0",
    "@angular/localize": "^19.1.4",
    "@briebug/jest-schematic": "^6.0.0",
    "@types/jest": "29.5.14",
    "@types/sockjs-client": "^1.5.4",
    "@types/stompjs": "^2.3.9",
    "jest": "^29.7.0",
    "replace-in-file": "^8.3.0",
    "typescript": "~5.7.2"
  }
}
